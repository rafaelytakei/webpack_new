{
  "parser": "babel-eslint",
	"extends": ["esnext", "plugin:import/errors", "plugin:import/warnings"],
	"settings": {
		"import/resolver": {
			"webpack": {
				"config": "config/webpack.common.js"
			}
		}
	},
	"env": {
		"es6": true,
		"commonjs": true,
		"node": true,
		"browser": true
	},
	"rules": {
		"babel/generator-star-spacing": "off",
		"no-console": "off",
		"no-useless-escape": "off",
		"eqeqeq": "off",
		"no-unused-vars": "warn",
		"semi":["error", "always"]
	}
}
